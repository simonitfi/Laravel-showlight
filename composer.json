{
  "name": "simonitfi/laravel-showlight",
  "keywords": [
    "laravel",
    "lumen"
  ],
  "type": "library",
  "require": {
    "php": ">=7.1",
    "illuminate/console": "^5.5|^6|^7|^8",
    "illuminate/support": "^5.5|^6|^7|^8"
  },
  "description": "Encrypter",
  "license": "MIT",
  "authors": [
    {
      "name": "Simonit",
      "email": "support@simonit.fi"
    }
  ],
  "autoload": {
    "psr-4": {
      "sbamtr\\LaravelSourceEncrypter\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "sbamtr\\LaravelSourceEncrypter\\SourceEncryptServiceProvider"
      ]
    }
  }
}
  
